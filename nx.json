{
  "plugins": ["@nxtensions/astro"],
  "npmScope": "frontmatter-cms-test",
  "namedInputs": {
    "default": ["{projectRoot}/**/*", "sharedGlobals"],
    "production": ["default"],
    "sharedGlobals": []
